{
  "name": "coding-coach-agent",
  "version": "1.0.0",
  "description": "AI coding coach with RAG and memory",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc --noEmit",
    "vercel-build": "prisma generate && tsc --noEmit",
    "postinstall": "prisma generate",
    "start": "tsx src/server.ts",
    "ingest": "tsx src/jobs/ingest.ts",
    "migrate": "prisma migrate dev",
    "typecheck": "tsc --noEmit",
    "smoke-test": "tsx src/smoke-test.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "coding",
    "coach",
    "rag",
    "typescript"
  ],
  "author": "Beth Cartrette",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^2.2.14",
    "@octokit/rest": "^21.0.2",
    "@prisma/client": "^6.14.0",
    "@types/multer": "^2.0.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^5.0.0-beta.3",
    "figma-js": "^1.16.0",
    "multer": "^2.0.2",
    "octokit": "^4.1.0",
    "openai": "^5.12.2",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "prisma": "^6.14.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
